{
    "name": "{{ rackVersion }}",
    "config": {
        "main": "org.wseduc.rack.Rack",
        "port": 8031,
        {% if csrfToken %}"csrf-token": true,{% endif %}
        "auto-redeploy": false,
        "app-name": "Rack",
        "app-address": "/rack",
        "app-icon": "rack-large",
        "app-displayName": "Rack",
        "app-type" : "END_USER",
        "host": "https://{{ host }}",
        "ssl": true,
        {% if overrideTheme %} "override-theme": {}, {% endif %}
        "userbook-host": "https://{{ host }}",
        "integration-mode": "HTTP",
        "app-registry.port": 8012,
        "mode": "prod",
        "entcore.port": 8009,
        "publicConf": {
            "xiti": {
                "ID_SERVICE": {
                    "default": 1
                },
                "LIBELLE_SERVICE": {
                    "default": "STOCKAGE_PARTAGE"
                }
            }
        }
    }
}